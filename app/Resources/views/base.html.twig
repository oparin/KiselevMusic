<div id="wrapper">
<div class="wrapper-inner">

<!-- Top -->
<div id="top-row">
    <div class="row-container">
        <div class="container-fluid">
            <div id="top" class="row-fluid">
                <div class="moduletable navigation  span12">
                    <ul id="top-menu" class="sf-menu sf-js-enabled">
                        <li class="firstItem {% if app.request.attributes.get('_route') == 'kiselevmusic_home_home_index' %}current active{% endif %}">
                            <a href="{{ path('kiselevmusic_home_home_index') }}">Главная</a>
                        </li>
                        <li class="{% if app.request.attributes.get('_route') == 'kiselevmusic_audio_audio_poluver' %}current active{% endif %}">
                            <a href="{{ path('kiselevmusic_audio_audio_poluver') }}">Аудио</a>
                        </li>
                        <li class="{% if app.request.attributes.get('_route') == 'kiselevmusic_video_video_video' %}current active{% endif %}">
